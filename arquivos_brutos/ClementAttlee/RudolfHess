<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-a5e5248c-abf5-4afd-8264-f4a7d1749ee4'>
            
<p class="">38.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-47d43522-8979-4e63-a63e-16cecc31c9bc">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-47d43522-8979-4e63-a63e-16cecc31c9bc">
