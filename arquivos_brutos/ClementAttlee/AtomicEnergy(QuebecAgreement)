<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-55c712a5-1706-49dc-a1be-d43311da33e7'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-5d13af85-f16e-4577-a0f2-b31e5477d5bd">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-5d13af85-f16e-4577-a0f2-b31e5477d5bd">
