<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-c680dd2c-eb0d-470b-9427-544385bc6c80'>
            
<p class="">25.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-f00dc62b-91ba-4640-9ada-151c20788991">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-f00dc62b-91ba-4640-9ada-151c20788991">
