<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-07ab1d7f-ace1-448a-a6af-ec5a81dcb52e'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-d329bc06-bd71-4485-adb6-0f0edf3bb00b">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-d329bc06-bd71-4485-adb6-0f0edf3bb00b">
