<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-6a37dc7d-4da5-42d2-b637-9ff917224c7a'>
            
<p class="">11.59 a.m.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-7d7c6083-474e-4307-bc85-d3378b435e56">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-7d7c6083-474e-4307-bc85-d3378b435e56">
