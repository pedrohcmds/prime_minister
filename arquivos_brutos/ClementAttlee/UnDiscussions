<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-d8265130-a373-4cc9-ad27-23d58accac1c'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-8a610d7e-9a09-4a8e-922d-291358533e01">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-8a610d7e-9a09-4a8e-922d-291358533e01">
