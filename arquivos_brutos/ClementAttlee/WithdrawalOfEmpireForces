                           data-hop-popover="http://hansard.parliament.uk/Commons/1941-04-30/debates/49a8be28-d4c0-4f07-b0f9-30469fcf8f0b/WithdrawalOfEmpireForces#contribution-96315246-f2c6-44d1-9243-9a412cab7812">
