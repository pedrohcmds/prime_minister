<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-a9abc7f9-bc45-405f-b111-f52bf95a6260'>
            
<p class="">69.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-324edfc6-479a-4d3b-b396-4ebc8c7afee6">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-324edfc6-479a-4d3b-b396-4ebc8c7afee6">
