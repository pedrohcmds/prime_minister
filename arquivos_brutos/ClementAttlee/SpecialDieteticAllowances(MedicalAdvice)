        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-c1155bb7-81cc-4c50-b5a0-ad630af568f0'>
            
<p class="">71.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
