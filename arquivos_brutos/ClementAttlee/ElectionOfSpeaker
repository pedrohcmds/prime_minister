        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-68caf76c-a482-4dde-8678-a03ab0632c67">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-68caf76c-a482-4dde-8678-a03ab0632c67">
