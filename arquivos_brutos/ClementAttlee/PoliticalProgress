<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-ef39be8d-1016-450f-8848-cdf486ae0b39'>
            
<p class="">47.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-f52321d9-79df-451e-8dca-4a7ca57a0619">
    <div class="col-md-9 edit-fail-error">
