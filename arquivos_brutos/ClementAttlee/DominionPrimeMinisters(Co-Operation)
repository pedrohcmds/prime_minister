<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-f7c61b46-7f8f-4874-9b6b-88e4a9a7d956'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-76a02c13-7f30-48e2-a007-f6194198c405">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-76a02c13-7f30-48e2-a007-f6194198c405">
