<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-974cff1a-c96d-45b8-8efc-2e6e04811792'>
            
<p class="">11.45 p.m.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-508e5211-f23a-4c99-80a3-9c489054a019">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-508e5211-f23a-4c99-80a3-9c489054a019">
