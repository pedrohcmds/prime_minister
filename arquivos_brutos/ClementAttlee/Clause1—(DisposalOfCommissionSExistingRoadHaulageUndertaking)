                           data-hop-popover="http://hansard.parliament.uk/Commons/1952-12-03/debates/b65528ba-60f0-4070-8e8a-d9c70085ca9c/Clause1%E2%80%94(DisposalOfCommissionSExistingRoadHaulageUndertaking)#contribution-cab777d8-a9e4-4b53-bd5b-f85d84bbf323">
