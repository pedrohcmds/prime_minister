               data-hop-url-shorten-url="/UrlShortener/ShorternUrl"
               data-hop-popover="http://hansard.parliament.uk/Commons/1951-06-11/debates/b8e9c5ff-92ac-4109-b376-e309fc15b420/Clause27%E2%80%94(EffectOfCapitalisationOfProfitsOnRateOfProfitsTax)#contribution-58f310aa-3756-432b-9a3b-5bc380a651e4">
