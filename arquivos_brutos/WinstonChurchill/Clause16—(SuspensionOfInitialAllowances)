<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-66f64282-cfc1-445f-ab00-c4618eccad30'>
            
<p class="">12.45 a.m.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-15029772-49fb-454f-8288-e0d10d785370">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-15029772-49fb-454f-8288-e0d10d785370">
