<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-d78092aa-bd6e-4167-ac16-787ae273ef55'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-d27005f2-2f03-4d61-a09f-408cae6f1be8">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-d27005f2-2f03-4d61-a09f-408cae6f1be8">
