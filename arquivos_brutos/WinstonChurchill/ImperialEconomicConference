<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-c341cd37-9932-401f-82b9-484578ddf98d'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-baf22d65-039b-4892-aa49-1d6d71c679d6">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-baf22d65-039b-4892-aa49-1d6d71c679d6">
