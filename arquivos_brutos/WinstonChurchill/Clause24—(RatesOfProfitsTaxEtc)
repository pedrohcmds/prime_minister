                           data-hop-popover="http://hansard.parliament.uk/Commons/1951-06-11/debates/433eb2ec-be09-48d7-96bd-3315e7fc99f3/Clause24%E2%80%94(RatesOfProfitsTaxEtc)#contribution-797e254a-ea76-488c-b554-4e56c3d4f8bd">
