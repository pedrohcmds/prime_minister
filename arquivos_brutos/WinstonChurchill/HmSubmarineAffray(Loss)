               data-hop-popover="http://hansard.parliament.uk/Commons/1951-06-20/debates/482f5047-195a-4f7e-bd85-f7c48f85464d/HmSubmarineAffray(Loss)#contribution-b4a7c442-a3cf-41a2-a813-583ab6ac154e">
