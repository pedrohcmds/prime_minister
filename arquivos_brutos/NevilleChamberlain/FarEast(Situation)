<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-e1775f00-6c85-40f9-98e5-844ecb162950'>
            
<p class="">12.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-154e2d32-6ae0-4217-9d7a-65c33b8aa2c3">
