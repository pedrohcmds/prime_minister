<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-7fafd812-13d3-4311-b827-98b0e1c84542'>
            
<p class="">48.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-2e9f10af-49dc-4499-8256-e927c694de9f">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-2e9f10af-49dc-4499-8256-e927c694de9f">
