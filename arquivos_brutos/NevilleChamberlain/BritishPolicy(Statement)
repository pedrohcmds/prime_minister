        <div class="statement col-md-9  content-container " id='contribution-0361d82c-615c-49e7-b37d-622c1f3d6277'>
            
<p class="">7.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
