<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-2d69d70a-e713-4630-97b0-7dad81b0761a'>
            
<p class="">15.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-edfdee2e-4046-42af-b1ad-3f0968a8b337">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-edfdee2e-4046-42af-b1ad-3f0968a8b337">
