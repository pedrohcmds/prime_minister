<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-26b4f7af-315a-4689-8fcc-b7c1ba52eef4'>
            
<p class="">46.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-cb5be9f6-01f4-4353-969f-6c980e54d12d">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-cb5be9f6-01f4-4353-969f-6c980e54d12d">
