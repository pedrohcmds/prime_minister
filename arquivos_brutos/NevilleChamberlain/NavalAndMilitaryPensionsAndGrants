<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-fe417960-cdf0-4986-910a-f54e43ac7227'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-9b2aca97-7901-47c9-ba16-eaf00a08aaed">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-9b2aca97-7901-47c9-ba16-eaf00a08aaed">
