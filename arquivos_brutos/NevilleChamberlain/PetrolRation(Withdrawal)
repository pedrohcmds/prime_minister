<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-c181f65d-edfa-4015-8aa9-680f8dcf4429'>
            
<p class="">36.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-12e50f62-8c38-4151-8466-28b9a0843dc5">
