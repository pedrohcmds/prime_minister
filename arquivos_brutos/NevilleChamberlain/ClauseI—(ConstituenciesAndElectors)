<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-5ec7a790-75ac-498c-8c8f-292326bdd707'>
            
<p class="">6.0 p.m.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-120d9344-b57b-4ce6-9cbc-3a6b044256d5">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-120d9344-b57b-4ce6-9cbc-3a6b044256d5">
