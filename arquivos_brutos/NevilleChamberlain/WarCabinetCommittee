<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-ba3dd995-baa5-4205-84ff-ca2fbfcc223f'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-fd18072f-fb30-4dcb-921b-383dde55b587">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-fd18072f-fb30-4dcb-921b-383dde55b587">
