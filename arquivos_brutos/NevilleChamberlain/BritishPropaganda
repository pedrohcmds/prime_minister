<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-078ef49a-d1fe-4274-88c4-9d0297ca1987'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-97e83d17-99f0-4c08-aad5-acec757e5f33">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-97e83d17-99f0-4c08-aad5-acec757e5f33">
