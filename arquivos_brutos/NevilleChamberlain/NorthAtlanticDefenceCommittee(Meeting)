<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-7a8f932c-11a7-4310-9626-9557304f0199'>
            
<p class="">52.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-1d304b54-9dfc-4df8-af39-eda9db28a770">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-1d304b54-9dfc-4df8-af39-eda9db28a770">
