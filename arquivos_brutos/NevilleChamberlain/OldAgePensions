<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-b84d8dc2-ffce-45d7-a33a-f8e55fef1eff'>
            
<p class="">45.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-3b130980-0a1f-4ac6-9805-8b7c8f88273b">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-3b130980-0a1f-4ac6-9805-8b7c8f88273b">
