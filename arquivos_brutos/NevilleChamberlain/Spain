        <div class="statement col-md-9  content-container " id='contribution-780982a2-81df-41c0-82dc-31651626e08a'>
            
<p class="">18.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-95e9c7e6-2c67-47a6-af70-7bcb03b66b14">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-95e9c7e6-2c67-47a6-af70-7bcb03b66b14">
