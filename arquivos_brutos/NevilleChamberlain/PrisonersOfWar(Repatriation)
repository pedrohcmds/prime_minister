<div class="content-item other-content">
        <!-- START statement -->
        <div class="statement col-md-9  content-container " id='contribution-70d2244c-0997-4467-921e-1c080ac0ac69'>
            
<p class="">64.</p>
        </div>
            <div class="col-md-3 hidden-sm hidden-xs right-column ">
            </div>
        <div class="clearfix"></div>
        <!-- END statement -->
</div><div class="content-item" id="contribution-36d90a58-0ca2-4811-bd8f-028c10caa270">
    <div class="col-md-9 edit-fail-error">
        <div class="alert alert-danger" id="alert-36d90a58-0ca2-4811-bd8f-028c10caa270">
